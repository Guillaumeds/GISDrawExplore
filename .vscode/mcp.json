{
  "inputs": [
    {
      "type": "promptString",
      "id": "github-token",
      "description": "GitHub Personal Access Token (optional)",
      "password": true
    }
  ],
  "servers": {
    "browser-tools": {
      "type": "stdio",
      "command": "npx",
      "args": ["@agentdeskai/browser-tools-mcp@1.2.0"]
    },
    "vscode-mcp-server": {
      "type": "stdio",
      "command": "npx",
      "args": ["mcp-remote@next", "http://localhost:3002/mcp"]
    },
    "fetch": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "mcp-server-fetch"]
    },
    "filesystem": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "mcp-server-filesystem", "${workspaceFolder}"]
    }
  }
}
